<!DOCTYPE html>
<html>
<head>
  <title>MusicApp</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if current_user %>
  <%= button_to "Log out", session_url, method: :delete %>
  <% else %>
  <%= link_to "Log In", new_session_url %>
  <%= link_to "Register", new_user_url %>
  <% end %>


<%= yield %>

</body>
</html>
